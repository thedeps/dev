<div class="page-header">
  <h2>Detalhes da Partida</h2>
</div>

<dl class="dl-horizontal">
  <dt>Team1:</dt>
  <dd><%= @match.team1.name %></dd>

  <dt>Team2:</dt>
  <dd><%= @match.team2.name %></dd>

  <dt>Date match:</dt>
  <dd><%= @match.date_match %></dd>

  <dt>Link:</dt>
  <dd><%= @match.link %></dd>

</dl>

<%= link_to matches_path, :class => 'btn btn-default' do %>
  <span class="glyphicon glyphicon-list-alt"></span>
  Voltar
<% end %>

